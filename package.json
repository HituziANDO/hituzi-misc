{
  "name": "hituzi-misc",
  "version": "0.0.13",
  "description": "JavaScript & TypeScript misc library.",
  "main": "dist-cjs/index.js",
  "module": "dist-cjs/index.js",
  "browser": {
    "crypto": false
  },
  "types": "dist-cjs/index.d.ts",
  "scripts": {
    "prepare": "./prepare.sh",
    "build": "npm-run-all -s build:cjs build:esm",
    "build:cjs": "rm -rf dist-cjs && tsc --project . --module commonjs --outDir ./dist-cjs",
    "build:esm": "rm -rf dist-esm && tsc --project . --module es2015 --outDir ./dist-esm",
    "test": "jest",
    "lint:fix": "eslint --fix ./src/**/*.ts ./test/**/*.ts",
    "doc": "typedoc --entryPointStrategy expand ./src && open ./docs/index.html",
    "prepublishOnly": "npm test && npm run build"
  },
  "author": "Hituzi Ando",
  "repository": {
    "type": "git",
    "url": "git@github.com:HituziANDO/hituzi-misc.git"
  },
  "homepage": "https://github.com/HituziANDO/hituzi-misc",
  "license": "MIT",
  "dependencies": {
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.7"
  },
  "lint-staged": {
    "./src/**/*.ts": [
      "eslint --fix"
    ],
    "./test/**/*.ts": [
      "eslint --fix"
    ]
  }
}
