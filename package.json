{
  "name": "hituzi-misc",
  "version": "0.0.4",
  "description": "JavaScript & TypeScript misc library.",
  "main": "dist-cjs/index.js",
  "module": "dist-esm/index.js",
  "types": "dist-cjs/index.d.ts",
  "scripts": {
    "prepare": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "build": "npm-run-all -s build:cjs build:esm",
    "build:cjs": "tsc --project . --module commonjs --outDir ./dist-cjs",
    "build:esm": "tsc --project . --module es2015 --outDir ./dist-esm",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:fix": "eslint --fix ./src/**/*.ts",
    "doc": "typedoc --entryPointStrategy expand ./src && open ./docs/index.html",
    "prepublishOnly": "npm run build"
  },
  "author": "Hituzi Ando",
  "repository": {
    "type": "git",
    "url": "git@github.com:HituziANDO/hituzi-misc.git"
  },
  "homepage": "https://github.com/HituziANDO/hituzi-misc",
  "license": "MIT",
  "dependencies": {
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typedoc": "^0.24.7"
  },
  "lint-staged": {
    "./src/**/*.ts": [
      "eslint --fix"
    ]
  }
}
